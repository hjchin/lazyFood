CREATE TABLE Food(
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    imagePath TEXT NOT NULL
);

insertFood:
INSERT INTO Food(imagePath)
VALUES(?);

removeFoodById:
DELETE FROM Food
WHERE id = ?;

selectAllFood:
SELECT *
FROM Food;

selectSortedFood:
SELECT *
FROM Food
ORDER BY id DESC;

selectLastInsertedFood:
SELECT last_insert_rowid();